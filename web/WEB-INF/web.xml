<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>csv.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>CSVServlet</servlet-name>
        <servlet-class>com.EmployeeInfoConvert.fs.web.servlet.CSVServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CSVServlet</servlet-name>
        <url-pattern>/csv.action</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EmailServlet</servlet-name>
        <servlet-class>com.EmployeeInfoConvert.fs.web.servlet.EmailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmailServlet</servlet-name>
        <url-pattern>/email.action</url-pattern>
    </servlet-mapping>
</web-app>